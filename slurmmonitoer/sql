CREATE TABLE IF NOT EXISTS `job` (
  `id` integer primary key auto_increment not null,
  `jobid` integer not null,
  `jobname` varchar(256),
  `queue` varchar(64) not null,
  `qtime` integer not null,
  `starttime` integer not null,
  `endtime` integer not null,
  `submiter` varchar(256) not null,
  `jobstatus` varchar(32) not null,
  `nodescount` int not null,
  `cpuscount` int not null,
  `tres_text` varchar(256),
  `nodes`   varchar(256) not null,
  `memory`  integer,
  `gpus` int,
  `errput` varchar(256),
  `output` varchar(256),
  `command` varchar(256));